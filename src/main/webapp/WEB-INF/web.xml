<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  <servlet>
  	<servlet-name>SignIn</servlet-name>
  	<display-name>SignIn</display-name>
  	<description></description>
  	<servlet-class>com.SpiritPokemon.Controller.SignIn</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>SignUp</servlet-name>
  	<display-name>SignUp</display-name>
  	<description></description>
  	<servlet-class>com.SpiritPokemon.Controller.SignUp</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>Pokemon</servlet-name>
  	<display-name>Pokemon</display-name>
  	<description></description>
  	<servlet-class>com.SpiritPokemon.Controller.Pokemon</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>Pokedex</servlet-name>
  	<display-name>Pokedex</display-name>
  	<description></description>
  	<servlet-class>com.SpiritPokemon.Controller.Pokedex</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>SignIn</servlet-name>
  	<url-pattern>/SignIn</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>SignUp</servlet-name>
  	<url-pattern>/SignUp</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>Pokemon</servlet-name>
  	<url-pattern>/Pokemon</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>Pokedex</servlet-name>
  	<url-pattern>/Pokedex</url-pattern>
  </servlet-mapping>
</web-app>
